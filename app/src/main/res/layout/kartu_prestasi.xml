<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardCornerRadius="30dp"
    android:orientation="vertical">

    <LinearLayout
        android:background="@color/colorOrange"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imgLogo"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_weight="3"
                android:src="@drawable/pnp"></ImageView>

            <TextView
                android:id="@+id/txtJudul"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:fontFamily="@font/vbold"
                android:gravity="center"
                android:src="@drawable/pnp"
                android:text="@string/data_prestasi_sd_30"
                android:textColor="@color/colorBlack"
                android:textSize="18sp"></TextView>
        </LinearLayout>
        <View
            android:layout_margin="8dp"
            android:layout_marginTop="8dp"
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="4dp"
            android:background="@color/colorBlack" />
        <TableLayout
            android:layout_margin="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TableRow>

                <TextView
                    android:id="@+id/idPrestasi"
                    android:fontFamily="@font/vregular"
                    android:text="Id Prestasi"
                    android:textColor="@color/colorBlack"></TextView>
                <TextView
                    android:text=" : "></TextView>

                <TextView
                    android:id="@+id/txtIdPrestasi"
                    android:fontFamily="@font/vregular"
                    android:text="Id Prestasi"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>
            <TableRow>

                <TextView
                    android:id="@+id/idUser"
                    android:fontFamily="@font/vregular"
                    android:text="@string/id_user"
                    android:textColor="@color/colorBlack"></TextView>
                <TextView
                    android:text=" : "></TextView>

                <TextView
                    android:id="@+id/txtIdUser"
                    android:fontFamily="@font/vregular"
                    android:text="@string/id_user"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>
            <TableRow>

                <TextView
                    android:id="@+id/jenisPrestasi"
                    android:fontFamily="@font/vregular"
                    android:text="@string/jenis_prestasi"
                    android:textColor="@color/colorBlack"></TextView>
                <TextView
                    android:text=" : "></TextView>

                <TextView
                    android:id="@+id/txtJenisPrestasi"
                    android:fontFamily="@font/vregular"
                    android:text="@string/jenis_prestasi"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>
            <TableRow>

                <TextView
                    android:id="@+id/tingkat"
                    android:fontFamily="@font/vregular"
                    android:text="@string/tingkat"
                    android:textColor="@color/colorBlack"></TextView>
                <TextView
                    android:text=" : "></TextView>

                <TextView
                    android:id="@+id/txtTingkat"
                    android:fontFamily="@font/vregular"
                    android:text="@string/tingkat"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>
            <TableRow>

                <TextView
                    android:id="@+id/prestasi"
                    android:fontFamily="@font/vregular"
                    android:text="Prestasi"
                    android:textColor="@color/colorBlack"></TextView>

                <TextView android:text="1"></TextView>

                <TextView
                    android:id="@+id/txtNamaPrestasi1"
                    android:fontFamily="@font/vregular"
                    android:text="Nama Prestasi 1"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>
            <TableRow>

                <TextView
                    android:fontFamily="@font/vregular"
                    android:textColor="@color/colorBlack"></TextView>

                <TextView></TextView>

                <TextView
                    android:id="@+id/txtTahun1"
                    android:fontFamily="@font/vregular"
                    android:text="Tahun 1"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>
            <TableRow>

                <TextView
                    android:fontFamily="@font/vregular"
                    android:textColor="@color/colorBlack"></TextView>

                <TextView></TextView>

                <TextView
                    android:id="@+id/txtPenyelenggara1"
                    android:fontFamily="@font/vregular"
                    android:text="Penyelenggara 1"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>

            <TableRow>

                <TextView
                    android:fontFamily="@font/vregular"
                    android:textColor="@color/colorBlack"></TextView>

                <TextView android:text="2"></TextView>

                <TextView
                    android:id="@+id/txtNamaPrestasi2"
                    android:fontFamily="@font/vregular"
                    android:text="Nama Prestasi 2"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>
            <TableRow>

                <TextView
                    android:fontFamily="@font/vregular"
                    android:textColor="@color/colorBlack"></TextView>

                <TextView></TextView>

                <TextView
                    android:id="@+id/txtTahun2"
                    android:fontFamily="@font/vregular"
                    android:text="Tahun 2"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>
            <TableRow>

                <TextView
                    android:fontFamily="@font/vregular"
                    android:textColor="@color/colorBlack"></TextView>

                <TextView></TextView>

                <TextView
                    android:id="@+id/txtPenyelenggara2"
                    android:fontFamily="@font/vregular"
                    android:text="Penyelenggara 2"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>

            <TableRow>

                <TextView
                    android:fontFamily="@font/vregular"
                    android:textColor="@color/colorBlack"></TextView>

                <TextView android:text="3"></TextView>

                <TextView
                    android:id="@+id/txtNamaPrestasi3"
                    android:fontFamily="@font/vregular"
                    android:text="Nama Prestasi 3"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>
            <TableRow>

                <TextView
                    android:fontFamily="@font/vregular"
                    android:textColor="@color/colorBlack"></TextView>

                <TextView></TextView>

                <TextView
                    android:id="@+id/txtTahun3"
                    android:fontFamily="@font/vregular"
                    android:text="Tahun 3"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>
            <TableRow>

                <TextView
                    android:fontFamily="@font/vregular"
                    android:textColor="@color/colorBlack"></TextView>

                <TextView></TextView>

                <TextView
                    android:id="@+id/txtPenyelenggara3"
                    android:fontFamily="@font/vregular"
                    android:text="Penyelenggara 3"
                    android:textColor="@color/colorBlack"></TextView>
            </TableRow>
        </TableLayout>
    </LinearLayout>

</androidx.cardview.widget.CardView>